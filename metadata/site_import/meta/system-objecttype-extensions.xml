    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="nuveiEnabled">
                <display-name xml:lang="x-default">Enabled</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiEnvironment">
                <display-name xml:lang="x-default">Environment</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Test</display>
                        <value>test</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Production</display>
                        <value>prod</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiMerchantID">
                <display-name xml:lang="x-default">Merchant ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiMerchantName">
                <display-name xml:lang="x-default">Merchant Name</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiMerchantPhone">
                <display-name xml:lang="x-default">Merchant Phone</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiMerchantSecretKey">
                <display-name xml:lang="x-default">Merchant Secret Key</display-name>
                <type>password</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiMerchantSiteID">
                <display-name xml:lang="x-default">Merchant Site ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiRedirectMode">
                <display-name xml:lang="x-default">Redirect Mode</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Hosted Page</display>
                        <value>Hosted Page</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Direct</display>
                        <value>Direct</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiRedirectType">
                <display-name xml:lang="x-default">Redirect Type</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">iFrame</display>
                        <value>iFrame</value>
                    </value-definition>
                    <value-definition default="true">
                        <display xml:lang="x-default">redirect</display>
                        <value>redirect</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="nuveiTransactionType">
                <display-name xml:lang="x-default">Transaction Type</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">Sale – Performs a regular payment request</display>
                        <value>Sale</value>
                    </value-definition>
                    <value-definition default="true">
                        <display xml:lang="x-default">Auth – Performs an authorization-only request</display>
                        <value>Auth</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">PreAuth – Performs an authorization-only request (with a longer settlement period)</display>
                        <value>PreAuth</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
			 <attribute-definition attribute-id="nuveiIsNative">
                <display-name xml:lang="x-default">Force APM to redirect</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
			<attribute-definition attribute-id="nuveiVersion">
                <display-name xml:lang="x-default">Plugin Version</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>21.1.0</default-value>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Nuvei Configuration">
                <display-name xml:lang="x-default">Nuvei Configuration</display-name>
                <attribute attribute-id="nuveiEnabled"/>
                <attribute attribute-id="nuveiEnvironment"/>
                <attribute attribute-id="nuveiMerchantID"/>
                <attribute attribute-id="nuveiMerchantSiteID"/>
                <attribute attribute-id="nuveiMerchantSecretKey"/>
                <attribute attribute-id="nuveiTransactionType"/>
                <attribute attribute-id="nuveiRedirectMode"/>
                <attribute attribute-id="nuveiRedirectType"/>
                <attribute attribute-id="nuveiMerchantName"/>
                <attribute attribute-id="nuveiMerchantPhone"/>
				<attribute attribute-id="nuveiIsNative"/>
				<attribute attribute-id="nuveiVersion"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

</metadata>
